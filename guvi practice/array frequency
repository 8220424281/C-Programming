#include<stdio.h>
#include<string.h>
int main(void)
{
	int i;
	int j;
	int count=1;
	char a[10];
	scanf("%s",a);
	for(i=0;i<strlen(a);i++)
	{
		count=1;
		for(j=i+1;j<strlen(a);j++)
		{
			if(a[i]==a[j])
			{
				count++;
			
			}
		}
                        if(count>1)
		{
			a[i]=a[++i];
			
		}
	
		printf("\n%c is %d",a[i],count);
	}
}
	
