#include <stdio.h>
#include<string.h>
int main(void) {
	// your code goes here
	char a[10];
	scanf("%s",a);
	int i;
	char s[10];
	for(i=strlen(a)-1;i>=0;i--)
	{
            if(a[i]=='a'||a[i]=='e'||a[i]=='i'||a[i]=='o'||a[i]=='u'||a[i]=='A'||a[i]=='E'||a[i]=='I'||a[i]=='O'||a[i]=='U')
	
		{
			s[i]=a[i];
		
		}
		else
		printf("%c",a[i]);
		
	}
	

	return 0;
}
