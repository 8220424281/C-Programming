#include <stdio.h>
int main(void) {
	// your code goes here
	char a[10];
	scanf("%s",a);

	int i;
	
	int count=0;
	int count1=0;
	
	for(i=0;a[i]!='\0';i++)
	{
	
	
		if((a[i]>=65&&a[i]<=90)||(a[i]>=97&&a[i]<=122))
		{
			count++;
		}
	if(a[i]>='0'&&a[i]<='9')
	{
		count1++;
	}
	
	}
	printf("\n%d",count);
	printf("\n%d",count1);
	

	return 0;
}
